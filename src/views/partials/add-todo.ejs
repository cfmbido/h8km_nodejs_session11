<div class="add-todo">
  <h5 class="add-todo-heading">Add a todo.</h5>

  <form class="add-todo-form" method="POST" action="/todo" onclick="addTodo();">
    <div class="form-group form-title">
      <label> Title </label>
      <input
        id="title"
        type="text"
        class="form-control"
        name="title"
        placeholder="What do you wannt to do?"
      />
    </div>
    <div class="form-group form-submit">
      <button type="submit" class="btn btn-primary">Add Todo</button>
    </div>
  </form>
</div>
